<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="MinimumMauiProjectExample.AppShellNotAuth"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MinimumMauiProjectExample.Views"
    xmlns:m="clr-namespace:MinimumMauiProjectExample"
    Shell.FlyoutBehavior="Flyout">
    <ShellContent
        Title="Login Page"
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="LoginPage" />

    <ShellContent
        Title="Register Page"
        ContentTemplate="{DataTemplate local:RegisterPage}"
        Route="RegisterPage" />

    <ShellContent
        Title="MainPage"
        ContentTemplate="{DataTemplate m:MainPage}"
        Route="MainPage" />


</Shell>
